{
    "presets": [
        "react",
        ["env",{
            "targets": {
                "browsers":"last 2 version"
            },
            "loose":true,
            "modules":false
        }]
    ],
    "plugins": [
        "react-hot-loader/babel",
        "babel-plugin-transform-class-properties"
    ],
    "env": {
        "test":{
            "plugins":[
                "babel-plugin-transform-es2015-modules-commonjs"
            ]
        }
    }
}